<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="800.0">
         <children>
            <ImageView fitHeight="90.0" fitWidth="660.0" layoutX="71.0" layoutY="90.0" pickOnBounds="true">
               <image>
                  <Image url="@Picture/Base/Dungeon%20programming(TEXT).png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <AnchorPane layoutX="200.0" layoutY="209.0" prefHeight="300.0" prefWidth="400.0">
         <children>
            <VBox alignment="CENTER" layoutX="150.0" layoutY="39.0" prefHeight="200.0" prefWidth="100.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#Play" text="Играть">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="BLevelEditor" mnemonicParsing="false" onAction="#OpenLevelEditorSettings" text="Редактор уровней">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="BSettings" mnemonicParsing="false" onAction="#SettingsWindowOpen" text="Настройки">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="BExit3" mnemonicParsing="false" onAction="#Exit" text="Выход">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </children>
      </AnchorPane>
      <AnchorPane layoutY="492.0" prefHeight="108.0" prefWidth="73.0">
         <children>
            <ImageView fitHeight="75.0" fitWidth="81.0" layoutY="29.0" onMouseClicked="#BackLogin" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Picture/LevelPieces/Undercover-icon-transp.png" />
               </image></ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
